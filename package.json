{
  "name": "yatra-backend",
  "version": "2.0.0",
  "description": "Yatra Event Management System - Production-Ready Backend API with TypeScript, Security, and Structured Logging",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "dev": "npm run start:dev",
    "build": "nest build",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "start:preview": "cross-env NODE_ENV=preview node dist/src/main.js",
    "dev:preview": "cross-env NODE_ENV=preview nest start --watch",
    "deploy": "antigravity deploy --env=production",
    "deploy:preview": "antigravity deploy --env=preview",
    "migration:run": "npm run build && typeorm migration:run -d dist/typeorm-cli.js",
    "migration:revert": "npm run build && typeorm migration:revert -d dist/typeorm-cli.js",
    "start:express": "node src/server.js",
    "dev:express": "nodemon src/server.js",
    "type-check": "tsc --noEmit",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:setup": "sequelize-cli db:create && npm run db:migrate && npm run db:seed",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "yatra",
    "pilgrimage",
    "event-management",
    "mysql",
    "sequelize",
    "typescript",
    "security",
    "production-ready"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.16.0",
    "pino": "^8.17.2",
    "pino-http": "^8.6.1",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.35.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.28",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.28",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.7",
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.2",
    "rimraf": "^6.1.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}