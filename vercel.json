{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production",
    "CORS_ORIGIN": "http://localhost:3000,http://localhost:3001,http://localhost:5000,http://127.0.0.1:3000,https://yatra-kappa.vercel.app/"
  }
}
